{
  "name": "wrapper-spotify-api",
  "version": "1.0.0",
  "description": "A wrapper to work  the Spotify API (https://developer.spotify.com/)",
  "main": "src/index.js",
  "scripts": {
    "clear": "rimraf lib",
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "build:umd": "./node_modules/.bin/webpack",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test",
    "babel-version": "babel --version"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allansuru/wrapper-spotify"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "SuRu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allansuru"
  },
  "homepage": "https://https://github.com/allansuru",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "babel-cli": "^6.24.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.0-beta.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.1",
    "husky": "^0.11.9",
    "mocha": "^3.2.0",
    "node-fetch": "^2.6.1",
    "nyc": "^10.2.0",
    "rimraf": "^2.7.1",
    "sinon": "^9.2.0",
    "sinon-chai": "^3.5.0",
    "sinon-stub-promise": "^4.0.0",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  }
}